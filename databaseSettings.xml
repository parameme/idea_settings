<application>
  <component name="DatabaseSettings" version="5">
    <option name="reuse-result-tab" value="false" />
    <option name="always-review-parameters" value="false" />
    <option name="execute-outside-option" value="2" />
    <option name="execute-selection-option" value="3" />
    <exec-options>
      <exec-option new-tab="true" exec-outside="2" exec-selection="3" />
      <exec-option />
      <exec-option />
    </exec-options>
    <text-mode process-strings="true">
      <parameter-patterns>
        <parameter-pattern value="\?(\d+)" />
        <parameter-pattern value=":(\w+)" scope="*,-PostgreSQL" />
        <parameter-pattern value="%\w+" scope="JAVA,Python,PHP" predefined="%name" />
        <parameter-pattern value="$\{[^$\{\}]*\}" />
        <parameter-pattern value="$\w+$" />
        <parameter-pattern value="#(\w+)#" scope="XML" predefined="#name#" />
        <parameter-pattern value="\?(\d+)" in-literals="false" />
        <parameter-pattern value="\$\{([^\{\}]*)\}" in-literals="false" predefined="${name}" />
        <parameter-pattern value="\$\(([^\)]+)\)" in-literals="false" predefined="$(name)" />
        <parameter-pattern value="\$(\w+)\$" in-literals="false" scope="*,-PostgreSQL" />
        <parameter-pattern value="(?&lt;=\W|\A)\$(\d+|[a-zA-Z_](?:\w|\.)*)\$?(?=\W|\z)" in-literals="false" scope="*,-SQL" predefined="$a.b.c$?" />
        <parameter-pattern value="(?&lt;=\W|\A)#(\d+|[a-zA-Z_](?:\w|\.)*)#?(?=\W|\z)" in-literals="false" scope="*,-SQL" predefined="#a.b.c#?" />
        <parameter-pattern value="%\((\w+)\)s" in-literals="false" scope="Python" predefined="%(name)s" />
        <parameter-pattern value="&lt;%=?.*%&gt;" />
        <parameter-pattern value="#{name}" in-scripts="true" />
      </parameter-patterns>
    </text-mode>
    <csv-formats>
      <csv-format name="Pipe-separated" id="Pipe-separated_id">
        <data>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="|" recordSeparator="&#10;">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </data>
      </csv-format>
      <csv-format name="Tab Separated Values (TSV)" id="708138bc-fe36-4bd5-8fae-8cff186310f7">
        <data>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="always" valueSeparator="&#9;" recordSeparator="&#10;" trimWhitespace="true">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </data>
        <header>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="always" valueSeparator="&#9;" recordSeparator="&#10;" trimWhitespace="true">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </header>
      </csv-format>
      <csv-format name="Comma Separated Values (CSV)" id="10ce2bdc-6481-4413-a883-00412de63f6b">
        <data>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="always" valueSeparator="," recordSeparator="&#10;" trimWhitespace="true">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </data>
        <header>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="," recordSeparator="&#10;" trimWhitespace="true">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </header>
      </csv-format>
      <csv-format name="Pipe Separated Values (PSV)" id="95290056-e01c-4d88-b5a0-81dd9cb82ec4">
        <data>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="|" recordSeparator="&#10;" trimWhitespace="true">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </data>
      </csv-format>
    </csv-formats>
  </component>
</application>