<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="3" />
    <option name="IS_VIRTUAL_SPACE" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="SHOW_REFORMAT_DIALOG" value="false" />
  </component>
  <component name="RubyEditorOptions">
    <option name="startInterpolationOnHash" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern icon="important" useCustomColors="false" case-sensitive="false" pattern="\bap\b">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern icon="important" useCustomColors="false" case-sensitive="false" pattern="\bbinding.pry\b">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <filter name="TODO">
      <pattern index="0" />
      <pattern index="1" />
    </filter>
    <filter name="Debugging">
      <pattern index="2" />
      <pattern index="3" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="breadcrumbsEnabledInXml" value="true" />
  </component>
</application>