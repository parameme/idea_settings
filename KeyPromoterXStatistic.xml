          <IdeaActionID>railways.UpdateRoutesList</IdeaActionID>
          <ShortCut>'⇧⌘\'</ShortCut>
          <Description>Cancel route list update</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
