<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Cancel route list update">
        <Item>
          <IdeaActionID>railways.UpdateRoutesList</IdeaActionID>
          <ShortCut>'⇧⌘\'</ShortCut>
          <Description>Cancel route list update</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Console...">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.Any</IdeaActionID>
          <ShortCut>'⇧⌘F10'</ShortCut>
          <Description>Jump to Console...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'⇧⌘N'</ShortCut>
          <Description>New Scratch File</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="RSpec 'let'">
        <Item>
          <IdeaActionID>rspec.LetIntroduce</IdeaActionID>
          <ShortCut>'⌥⇧⌘R' or '⇧⌘L'</ShortCut>
          <Description>RSpec 'let'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Run spec 'custom_amount_value_spec': envision'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'Run spec 'custom_amount_value_spec': envision'</Description>
          <Count>23</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Run spec 'grouped_progress_summary_sql_spec': envision'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'Run spec 'grouped_progress_summary_sql_spec': envision'</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Run spec 'grouped_pr...'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'Run spec 'grouped_pr...'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Run spec 'grouped_progress_summary_sql_spec': envision'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Run spec 'grouped_progress_summary_sql_spec': envision'</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>