      </Statistic>
      <Statistic Action="Rerun 'Run spec 'grouped_progress_summary_sql_spec': envision'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'Run spec 'grouped_progress_summary_sql_spec': envision'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Run spec 'grouped_pr...'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'Run spec 'grouped_pr...'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Run spec 'grouped_progress_summary_sql_spec': envision'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Run spec 'grouped_progress_summary_sql_spec': envision'</Description>
          <Count>2</Count>
